{
  "manifest_version": 2,

  "name": "Open At",
  "description": "This extension helps you open urls in different domains",
  "version": "1.0",

  "permissions": [
   "contextMenus", "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["goto.js"]
  },
  "icons": {
    "128": "glyphicons_151_new_window.png"
  },
  "options_page": "options.html"
}

